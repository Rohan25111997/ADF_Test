{
	"name": "dataflow1",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "Source_ADLS_DS",
						"type": "DatasetReference"
					},
					"name": "SalesInput"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "SnowflakeTable1",
						"type": "DatasetReference"
					},
					"name": "Snowflake"
				}
			],
			"transformations": [
				{
					"name": "Filtering"
				},
				{
					"name": "aggregation"
				}
			],
			"scriptLines": [
				"source(output(",
				"          ORDERNUMBER as short,",
				"          QUANTITYORDERED as short,",
				"          PRICEEACH as double,",
				"          ORDERLINENUMBER as short,",
				"          SALES as double,",
				"          ORDERDATE as timestamp,",
				"          STATUS as string,",
				"          QTR_ID as short,",
				"          MONTH_ID as short,",
				"          YEAR_ID as short",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> SalesInput",
				"SalesInput filter(SALES >= 2000) ~> Filtering",
				"Filtering aggregate(groupBy(YEAR_ID,",
				"          MONTH_ID),",
				"     TOTALSALES = round(sum(SALES), 2)) ~> aggregation",
				"aggregation sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     input(",
				"          YEAR_ID as string,",
				"          MONTH_ID as string,",
				"          TOTALSALES as string",
				"     ),",
				"     deletable:false,",
				"     insertable:true,",
				"     updateable:false,",
				"     upsertable:false,",
				"     format: 'table',",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     stageInsert: true) ~> Snowflake"
			]
		}
	}
}